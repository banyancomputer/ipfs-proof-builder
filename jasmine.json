{
  "reporters": [
    {
      "name": "jasmine-spec-reporter#SpecReporter",
      "options": {
        "displayStacktrace": "all"
      }
    }
  ],
  "spec_dir": "spec",
  "spec_files": [
    "**/*[sS]pec.?(m)ts"
  ],
  "helpers_dir": "spec/helpers",
  "helpers": [
    "**/*.?(m)ts"
  ],
  "env": {
    "stopSpecOnExpectationFailure": false,
    "random": true
  }
}
